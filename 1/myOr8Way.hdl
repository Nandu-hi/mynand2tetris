/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP myOr8Way {
    IN in[8];
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    myOr(a=in[0],b=in[1],out=out1);
    myOr(a=in[2],b=in[3],out=out2);
    myOr(a=in[4],b=in[5],out=out3);
    myOr(a=in[6],b=in[7],out=out4);
    myOr(a=out1,b=out2,out=out21);
    myOr(a=out3,b=out4,out=out22);
    myOr(a=out21,b=out22,out=out);
    
}
