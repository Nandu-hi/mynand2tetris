/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP myDMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    //// Replace this comment with your code.
    myNot(in=sel,out=notsel);
    myAnd(a=in,b=notsel,out=a);
    myAnd(a=in,b=sel,out=b);
}
